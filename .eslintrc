{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "extends": [
    "airbnb",
    "plugin:vue/essential"
  ],
  "rules": {
    "no-param-reassign": "off",
    "import/prefer-default-export": "warn"
  },
  "env": {
    "shared-node-browser": true
  },
  "globals": {
    "ENV": true,
    "ENV_SERVER": true,
    "ENV_CLIENT": true
  }
}