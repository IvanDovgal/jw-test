{
  "ignore": [],
  "env": {
    "test": {
      "presets": [[
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]],
      "plugins": ["@babel/plugin-transform-runtime", "@babel/plugin-syntax-dynamic-import"]
    },
    "production": {
      "presets": [[
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": false
          }
        }
      ]],
      "plugins": ["@babel/plugin-transform-runtime", "@babel/plugin-syntax-dynamic-import"]
    },
    "development": {
      "presets": [[
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]],
      "plugins": ["@babel/plugin-transform-runtime", "@babel/plugin-syntax-dynamic-import"]
    }
  }
}